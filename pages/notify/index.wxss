/* pages/notify/index.wxss */
.items{
    width: 750rpx;
    height: 148rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 0 30rpx;
        
    background-color: #ffffff;
    margin-top: 20rpx;

}
.container {
  background-color: #F0F2F8;
}

.c-left{
    width: 80rpx;
    height: 80rpx;
    /* border-radius: 50%; */
    margin-right: 20rpx;
    display: block;

}

.c-right{
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    justify-content: space-between;
    /* width: 550rpx; */

}

.title{
    font-size: 28rpx;
    color: #555555;
}
.description{
    font-size: 24rpx;
    color: #797979;
}

.right{
  background: rgb(255,111,142);
  height: 148rpx;
  text-align: center;
  line-height: 148rpx;
}
.icon-del{
  /* display: block; */
  width: 38rpx;
  height: 38rpx;
  text-align: center;
  
}