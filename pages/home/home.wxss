/* pages/home/home.wxss */

.zhezhao {
  position: fixed;
  z-index: 999;
  width: 750rpx;
  height: 100%;
  background-color: rgba(231, 211, 211, 0);
}

.zhezhao:not([size='mini']) {
  width: 750rpx;
}

._title {
  padding-left: 40rpx;
  color: rgb(153, 153, 153);
  font-size: 28rpx;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  overflow: hidden;
  background-color: #fafafa;
}

.t_header {
  margin-top: 36rpx;
  height: 64rpx;
}

.header {
  position: fixed;
  height: 400rpx;
  width: 750rpx;
  background-repeat: no-repeat;
  background-size: 100% auto;
  z-index: 666;
}

.notify {
  height: 70rpx;
 
  width: 580rpx;
  background: rgb(0,0,0,0.8);
  font-size: 28rpx;
  color: rgb(255,255,255);
  line-height: 70rpx;
  border-radius: 50rpx;
  margin: 6rpx 0 10rpx 42rpx
}
.notify view{
  padding-left: 10rpx;
  display: flex;
  align-items: center;

}
.notify image{
  display: block;
  width: 46rpx;
  height: 46rpx;
  margin-right: 18rpx;
}
.t_bottom {
  display: flex;
  height: 70rpx;
}

.t_left_bottom text {
  font-size: 30rpx;
  color: #fff;
}

.custom-navbar-top {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  justify-content: center;
}

#myVideo {
  width: 100%;
}

.top-arrow {
  width: 70rpx;
}

.change-space {
  height: 50rpx;
  font-size: 38rpx;
  color: #fff;
}

.t_title {
  font-size: 48rpx;
  color: #fff;
  margin-left: 40rpx;
}

.t_left_bottom {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 670rpx;
  margin: 0 auto;
}

.content_top {
  width: 100%;
  height: 20rpx;
  background-color: #f0f2f8;
}

.tops-header {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}

.content-tops {
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 0 30rpx;
  min-height: 150rpx;
  box-sizing: border-box;
}

.avatal {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.either-space {
  /* margin-top: 80rpx !important; */
}

.type-title {
  font-size: 28rpx;
  color: #999;
}

.type-description {
  font-size: 30rpx;
  color: #555;
}

.group-content {
  margin-top: 400rpx;
  width: 750rpx;
}

.content-header {
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
}

.img-l {
  width: 450rpx;
  height: 600rpx;
}

.img-l>image {
  width: 446rpx;
  height: 600rpx;
}

.t_button {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, 0.4);
}

.top-icon {
  width: 40rpx;
  height: 40rpx;
}

.goods-discription {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.goods-discription text {
  margin-left: 10rpx;
}

.img-r {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  width: 300rpx;
  height: 600rpx;
}

.img-r image {
  width: 100%;
  height: 299rpx;
}

.botton {
  width: 100%;
  max-height: 1000rpx;
  min-height: 400rpx;
  background-color: #fff;
}

.class-list {
  display: flex;
  flex-wrap: wrap;
  padding: 0 28rpx 40rpx 10rpx;
  margin-top: 18rpx;
  justify-content: space-between;
}

.class-content {
  width: 336rpx;
  height: 130rpx;
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  margin-top: 10rpx;
  border-radius: 20rpx;
  background-color: #eaedf3;
  margin-bottom: 20rpx;
}

.a-img {
  border-radius: 50%;
  margin-right: 10rpx;
  margin-left: 20rpx;
  width: 70rpx;
  height: 70rpx;
}

.exit-icon {
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  width: 34rpx;
  height: 34rpx;
}

.showSpaceList {
  width: 320rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.arrow-down {
  transform: rotate(180deg);
}

.content-bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
}

.bot-r {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.arrow-right {
  width: 50rpx;
  transform: rotate(270deg);
}

.arrow-up {
  transform: rotate(180deg);
}

.onUpImg {
  position: fixed;
  z-index: 555;
  border-radius: 50%;
  overflow: hidden;
  right: 30rpx;
}

.onUpImg>image {
  width: 148rpx;
  height: 148rpx;
}

.none-image {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.fangkuai {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 180rpx;
  height: 180rpx;
  background-color: #d4d6dd;
}

.fangkuai image {
  display: flex;
  width: 80rpx;
  height: 80rpx;
}

.del_space {
  position: relative;
  left: 480rpx;
  border-radius: 30rpx;
  overflow: hidden;
  width: 254rpx;
  height: 56rpx;
  background-color: rgba(0, 0, 0, 0.3);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 30rpx;
}

.del_space image {
  width: 40rpx;
  height: 40rpx;
}

.del_space text {
  font-size: 28rpx;
  margin-right: 6rpx;
  color: #fff;
}

.img-one {
  width: 100%;
}

.img-two {
  width: 100%;
  height: 700rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.img-two image {
  width: 100%;
  height: 50%;
  margin-bottom: 4rpx;
}

.bottom-text {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
}

.video {
  width: 750rpx;
  position: relative;
}

.mengceng {
  position: absolute;
  left: 0;
  top: 0;
  display: block;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 111;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.mengceng image {
  width: 74rpx;
  height: 74rpx;
  display: flex;
}

.video > image {
  width: 100%;
}

.add_zone {
  display: flex;
}

.add-icon {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  margin: 0 13rpx 0 20rpx;
}

.add-img {
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
}

button::after {
  border: none;
}

.add_A {
  font-size: 30rpx;
  color: rgb(153, 153, 153);
}

.prompt {
  width: 36rpx;
  height: 36rpx;
  text-align: center;
  font-size: 26rpx;
  background: rgb(248, 94, 125);
  color: #fff;
  border-radius: 100%;
  position: absolute;
  right: -5rpx;
  top: -5rpx;
}

.bot-l {
  margin-left: 20rpx;
}

.bot-l image {
  display: block;
  width: 40rpx;
  height: 40rpx;
}

.Leave_ {
  width: 710rpx;
  display: flex;
  background: pink;
  padding:20rpx;
}

.Leave_ view {
  width: 90rpx;
  text-align: center;
  line-height: 60rpx;
  background: red;
  font-size: 30rpx;
  color: #fff;
  height: 60rpx;
  border-radius: 15rpx;
}

.Leave_ textarea {
  height: 60rpx;
  max-height: 100rpx;
  font-size: 30rpx;
  margin-right: 30rpx;
  flex: 1;

}
