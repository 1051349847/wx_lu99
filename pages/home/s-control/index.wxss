/* pages/home/s-control/index.wxss */
.container{
    background-color: #EAEDF3;
}
.select-space{
    width: 100%;
    margin-top: 20rpx;
    background-color: #ffffff;
}

.delete-space{
    width: 100%;
    height: 88rpx;
    margin-top: 20rpx;
    background-color: #ffffff;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;

}
.delete-space text{
    font-size: 30rpx;
    text-align: center;
    color: #43C2D5;
}

.right-k{
    transform: rotate(270deg);
    height: 48rpx;
    width: 48rpx;
    margin-right: -10rpx;
}

.items{
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    height: 100rpx;
    box-sizing:border-box;
    padding: 0 30rpx;
}

.title{
    font-size: 32rpx;
    color: #555555;
}
.con-text{
    color: #999999;
}
.del{
  font-size: 24rpx;
  color: #AAAAAA;
  margin: 40rpx;


}