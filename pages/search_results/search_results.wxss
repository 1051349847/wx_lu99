/* pages/Shielding_photos/Shielding_photos.wxss */

.phonto_title {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: rgb(153, 153, 153);
  padding: 25rpx 30rpx;
}

.phonto_title image {
  display: block;
  width: 34rpx;
  height: 28rpx;
}

.phont_date {
  padding: 0 20rpx 0 15rpx;
}

.select-icon {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-content: center;
  justify-content: center;
  background-color: #fff;
}

.grid-item {
  /* height: 248rpx; */
  position: relative;
}

.selectBox {
  position: absolute;
  z-index: 999;
  right: 2rpx;
  top: 2rpx;
}

.checked {
  color: #3eace2;
  background: rgba(49, 165, 253, 0.08);
}

.img {
  width: 185rpx;
  height: 185rpx;
  display: block;
  margin-bottom: 6rpx;
}

.grid {
  width: 100%;
}
.operation{
display: flex;
justify-content: space-between;
font-size: 30rpx;
padding: 20rpx 20rpx;
height: 50rpx;
line-height: 50rpx;
position: fixed;
top: 0rpx;
z-index: 9999;
background: #fff;
width: 100%;
box-shadow: 0px 5rpx 5rpx 0px rgba(233, 236, 242);



}
.operation_ope{
  color: rgb(85,85,85);
  border:1px solid rgb(85,85,85);
  border-radius: 10rpx;
  text-align: center;

}
.operation_over{
  width: 100rpx;
 background: linear-gradient(to right, rgb(83,205,220), rgb(51,183,206));
 color: #fff;
border-radius: 10rpx;
text-align: center;


}

.data_N{
  display: block;
width: 300rpx;
height: 200rpx;
margin: 40rpx auto;

}
.data_text{
  text-align: center;
  font-size: 30rpx;
}